@{
    ViewBag.Title = "UserList";
}
@model List<UserViewModel>

<h2>Üye Listesi</h2>


<table class="table table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>Kullanýcý Adý</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Email</td>
            </tr>
        }
    </tbody>
</table>
